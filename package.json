{
  "name": "my-portfolio-v3",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider npx cross-env NODE_ENV=development vue-cli-service build --mode development",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:production": "npx cross-env NODE_ENV=production vue-cli-service build --mode production",
    "build:production-gitlab": "NODE_OPTIONS=--openssl-legacy-provider npx cross-env NODE_ENV=production vue-cli-service build --mode production",
    "build:staging": "NODE_OPTIONS=--openssl-legacy-provider npx cross-env NODE_ENV=staging vue-cli-service build --mode staging",
    "dev": "vue-cli-service serve --open",
    "inspect": "vue-cli-service inspect --mode",
    "lint:ts": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "p": "prettier --write 'src/**/*.js'",
    "pw": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/lodash": "^4.14.123",
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "vue": "^2.6.6",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^2.5.8",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@mdi/font": "^3.5.95",
    "@types/chart.js": "^2.9.35",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.9.1",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-plugin-unit-mocha": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.0.1",
    "chart.js": "^2.9.4",
    "deepmerge": "^4.2.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^27.5.1",
    "jest-mock-req-res": "^1.0.2",
    "sass": "~1.32",
    "sass-loader": "7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "typescript": "4.3.5",
    "vue-chartjs": "^3.5.1",
    "vue-clipboard2": "^0.3.3",
    "vue-template-compiler": "^2.5.21"
  }
}

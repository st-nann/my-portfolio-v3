{
  "name": "my-portfolio-v2",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "npx cross-env NODE_ENV=development vue-cli-service build --mode development",
    "build:staging": "npx cross-env NODE_ENV=staging vue-cli-service build --mode staging",
    "build:production": "npx cross-env NODE_ENV=production vue-cli-service build --mode production",
    "inspect": "vue-cli-service inspect --mode",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "p": "prettier --write 'src/**/*.js'",
    "pw": "onchange 'src/**/*.js' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "@types/chart.js": "^2.9.7",
    "@types/lodash": "^4.14.123",
    "axios": "^0.18.0",
    "chart.js": "^2.9.3",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "vue": "^2.6.6",
    "vue-chartjs": "^3.5.0",
    "vue-class-component": "^6.0.0",
    "vue-clipboard2": "^0.3.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.5.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@mdi/font": "^3.5.95",
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-plugin-unit-mocha": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.2.1",
    "vue-template-compiler": "^2.5.21"
  }
}
